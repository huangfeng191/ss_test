{
    "source": {
        "table": "dynamic_daily_business"
    },
    "queries": {
        "outFrequency": {
            "type": "date",
            "value": "day",
            "operate": "eq"
        },
        "sn": "eligibilityUpLoop"
    },

    "aggregate": [{
            "$group": {
                "_id": { "ts_code": 1 },
                "count": { "$sum": 1 }
            }
        },
        {
            "$match": {
                "count": { "gt": 3 }
            }
        }
    ],
    "out": {
        "type": "table",
        "table": {
            "nm": "dynamic_basic_business",
            "key": {
                "sn": 1,
                "outFrequency": 1
            }
        }
    }

}